{
  "name": "KhanCycle",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "pretest": "ln -snf ../ node_modules/KhanCycle && mv .babelrc.test .babelrc",
    "test:mocha": "mocha --require react-native-mock/mock.js --require ./src/mocks --compilers js:babel-core/register 'src/**/*.spec.js' || true",
