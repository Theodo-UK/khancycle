{
  "name": "KhanCycle",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test:mocha": "mocha --require react-native-mock/mock.js --require ./src/mocks --compilers js:babel-core/register 'src/**/*.spec.js'",
    "test:lint": "eslint 'src/**/*.js'",
    "test": "npm-run-all --continue-on-error --parallel test:*",
    "build:android": "npm install && cd ./android && ./gradlew assembleRelease",
    "build:ios": "cd ./ios && sh ./buildTools/buildArchive.sh",
    "postbuild:android": "mv ./android/app/build/outputs/apk/app-release.apk ./builds",
    "postbuild:ios": "mv ./ios/KhanCycle.ipa ./builds",
    "release": "npm-run-all --parallel build:*"
  },
  "dependencies": {
    "firebase": "^3.0.0",
    "geolib": "^2.0.21",
    "immutable": "^3.8.1",
    "moment": "^2.13.0",
    "react": "~15.0.2",
    "react-dom": "~15.0.2",
    "react-mixin": "^2.0.2",
    "react-native": "^0.26.2",
    "react-native-mapbox-gl": "^4.1.1",
    "react-native-router-flux": "^3.26.13",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "superagent": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-react-native": "^1.9.0",
    "chai": "^3.5.0",
    "enzyme": "^2.3.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "mocha": "^2.5.3",
    "npm-run-all": "^2.1.1",
    "react-addons-test-utils": "~15.0.2",
    "react-native-mock": "^0.2.2"
  }
}
